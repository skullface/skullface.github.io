<style>
  .logo {
    display: block;
    width: calc(var(--spacer) * 6);
    line-height: 1;
    svg {
      fill: var(--color-primary);
    }
  }

  .skull {
    .inverted {
      opacity: 0;
    }
    .standard {
      opacity: 1;
    }
  }

  @media (prefers-color-scheme: dark) {
    .skull {
      .inverted {
        opacity: 1;
        .sk-eye {
          fill: var(--color-background);
        }
      }
      .standard {
        opacity: 0;
      }
    }
  }

  html.dark .skull {
    .inverted {
      opacity: 1;
      .sk-eye {
        fill: var(--color-background);
      }
    }
    .standard {
      opacity: 0;
    }
  }

  html.light .skull {
    .inverted {
      opacity: 0;
    }
    .standard {
      opacity: 1;
    }
  }

  .sk-sparkle {
    transform-origin: center;
  }

  .sk-eye {
    transform-origin: 0 70px;
    animation: blink 4s cubic-bezier(0.65, 0.05, 0.36, 1) infinite;
  }

  .sk-sparkle-big {
    animation: float 4s ease-in-out infinite;
  }

  .sk-sparkle-small {
    animation: floatGrow 8s ease-in-out infinite;
  }

  .logo {
    transition: transform 130ms cubic-bezier(0.65, 0.05, 0.36, 1);
  }

  .logo:hover,
  .logo:focus {
    transform: scale(1.1);
  }
  .logo:hover .sk-sparkle-big,
  .logo:focus .sk-sparkle-big {
    animation: float 3s ease-in-out infinite;
  }
  .logo:hover .sk-sparkle-small,
  .logo:focus .sk-sparkle-small {
    animation: floatGrow 2s ease-in-out infinite;
  }

  @keyframes float {
    0%,
    33%,
    100% {
      opacity: 1;
    }
    66% {
      opacity: 0.5;
    }
  }

  @keyframes floatGrow {
    0%,
    50%,
    100% {
      opacity: 1;
    }
    30%,
    40% {
      opacity: 0.3;
    }
  }

  @keyframes blink {
    0%,
    10%,
    20%,
    100% {
      transform: scale(1, 1);
    }

    15% {
      transform: scale(1, 0.01);
    }
  }
</style>
